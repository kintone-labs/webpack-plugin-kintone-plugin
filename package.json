{
  "name": "@kintone/webpack-plugin-kintone-plugin",
  "version": "3.0.3",
  "description": "A webpack plugin to generate a plugin zip",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "release": "run-s build lint test && standard-version",
    "release:dryrun": "standard-version --dry-run",
    "unittest": "mocha --require ts-node/register --watch-extensions ts test/**/*-test.ts",
    "test": "run-s build unittest"
  },
  "keywords": [
    "webpack",
    "kintone"
  ],
  "files": [
    "dist/src"
  ],
  "author": "kintone",
  "license": "MIT",
  "repository": "kintone/webpack-plugin-kintone-plugin",
  "homepage": "https://github.com/kintone/webpack-plugin-kintone-plugin",
  "bugs": "https://github.com/kintone/webpack-plugin-kintone-plugin/issues",
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/chokidar": "^1.7.5",
    "@types/lodash": "^4.14.149",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^9.6.55",
    "@types/rimraf": "^2.0.3",
    "@types/webpack": "^4.41.0",
    "adm-zip": "^0.4.13",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "@kintone/plugin-packer": "^2.0.6",
    "chokidar": "^3.3.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1"
  }
}
