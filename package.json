{
  "name": "@kintone/webpack-plugin-kintone-plugin",
  "version": "1.1.0-alpha.0",
  "description": "A webpack plugin to generate a plugin zip",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "release": "run-s build lint test && standard-version",
    "release:dryrun": "standard-version --dry-run",
    "unittest": "mocha --require ts-node/register --watch-extensions ts test/**/*-test.ts",
    "test": "run-s build unittest"
  },
  "keywords": [
    "webpack",
    "kintone"
  ],
  "files": [
    "dist/src"
  ],
  "author": "kintone",
  "license": "MIT",
  "repository": "kintone/webpack-plugin-kintone-plugin",
  "homepage": "https://github.com/kintone/webpack-plugin-kintone-plugin",
  "bugs": "https://github.com/kintone/webpack-plugin-kintone-plugin/issues",
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/chokidar": "^1.7.5",
    "@types/lodash": "^4.14.130",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^9.6.48",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.32",
    "adm-zip": "^0.4.13",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "standard-version": "^5.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "@kintone/plugin-packer": "^1.1.0-alpha.0",
    "chokidar": "^2.1.6",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1"
  }
}
