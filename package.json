{
  "name": "@kintone/webpack-plugin-kintone-plugin",
  "version": "4.0.2",
  "description": "A webpack plugin to generate a plugin zip",
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "eslint src test --ext .ts",
    "release": "run-s build lint test && standard-version",
    "release:dryrun": "standard-version --dry-run",
    "unittest": "mocha --require ts-node/register --watch-extensions ts test/**/*-test.ts",
    "test": "run-s build unittest"
  },
  "keywords": [
    "webpack",
    "kintone"
  ],
  "files": [
    "dist/src"
  ],
  "author": "kintone",
  "license": "MIT",
  "repository": "kintone/webpack-plugin-kintone-plugin",
  "homepage": "https://github.com/kintone/webpack-plugin-kintone-plugin",
  "bugs": "https://github.com/kintone/webpack-plugin-kintone-plugin/issues",
  "devDependencies": {
    "@cybozu/eslint-config": "^10.0.3",
    "@types/adm-zip": "^0.4.33",
    "@types/chokidar": "^1.7.5",
    "@types/lodash": "^4.14.150",
    "@types/mkdirp": "^1.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^9.6.55",
    "@types/rimraf": "^3.0.0",
    "@types/webpack": "^4.41.12",
    "adm-zip": "^0.4.14",
    "eslint": "^6.8.0",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "@kintone/plugin-packer": "^3.0.3",
    "chokidar": "^3.4.0",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4"
  }
}
